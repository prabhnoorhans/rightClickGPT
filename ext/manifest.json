{
	"name": "rightClickGPT",
	"version": "1.0",
	"manifest_version": 3,
	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["main.js"],
			"css": ["styles.css"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"view.png",
				"mic.png"
			],
			"matches": ["<all_urls>"]
		}
	]
}